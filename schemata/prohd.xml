<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0" xml:lang="en">
  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>ODD del Proyecto Humboldt Digital</title>
        <editor>
          <persName>
            <surname>Rojas Castro</surname>
            <forename>Antonio</forename>
          </persName>
        </editor>
      </titleStmt>
      <publicationStmt>
        <publisher>ODD publicada por Proyecto Humboldt Digital (ProHD) por primera vez en enero de
            <date when="2021-01-15">2021</date></publisher>
      </publicationStmt>
      <sourceDesc>
        <bibl>ODD basada en TEI U5 y <ref
            target="https://www.ediarum.org/docs/ediarum.BASE.manual/index.html"
          />ediarum.BASE.manual</bibl>
      </sourceDesc>
    </fileDesc>
  </teiHeader>
  <text>
    <body>
      <schemaSpec ident="prohd" start="TEI" prefix="tei_" targetLang="es" docLang="es">

        <!-- modules -->
        <moduleRef key="tei"/>
        <moduleRef key="core"
          include="abbr add addrLine address author bibl biblScope choice cit corr date del desc divGen editor emph expan foreign gap gloss graphic head hi index item l label lb lg list listBibl measure mentioned milestone name note num orig p pb ptr pubPlace publisher q ref reg relatedItem resp respStmt rs sic soCalled sp speaker stage term time title unclear quote"/>
        <moduleRef key="header"
          include="authority availability catDesc catRef category change classCode classDecl creation distributor edition editionStmt editorialDecl encodingDesc extent fileDesc funder idno keywords langUsage language licence notesStmt principal profileDesc projectDesc publicationStmt refsDecl revisionDesc samplingDecl seriesStmt sourceDesc sponsor taxonomy teiHeader textClass titleStmt abstract listChange correspAction correspDesc correspContext"/>
        <moduleRef key="textstructure"
          include="TEI argument back body byline closer dateline div docAuthor docDate docEdition docImprint docTitle epigraph front group imprimatur opener postscript salute signed text titlePage titlePart trailer"/>
        <moduleRef key="figures" include="cell figure figDesc formula row table"/>
        <moduleRef key="linking" include="anchor seg"/>
        <moduleRef key="tagdocs" include="att code eg gi ident val"/>
        <moduleRef key="transcr" include="facsimile fw space subst ex supplied metamark"/>
        <moduleRef key="msdescription"
          include="physDesc msDesc msIdentifier institution collection repository"/>
        <moduleRef key="textcrit" include="listWit witness"/>
        <moduleRef key="namesdates" include="persName placeName surname forename orgName"/>
        <moduleRef key="gaiji" include="g"/>

        <!-- element Specifications -->

        <!-- elements in teiHeader -->
        <!-- required attributes and possible values -->
        <!-- graphic can be also in text -->
        <elementSpec ident="graphic" mode="change">
          <attList>
            <attDef ident="url" usage="opt" mode="replace"/>
            <attDef ident="facs" usage="opt" mode="replace"/>
            <attDef ident="hand" usage="opt" mode="replace">
              <valList type="semi" mode="replace">
                <valItem ident="#author"/>
                <valItem ident="#addressee"/>
                <valItem ident="#unknown"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="idno" mode="change">
          <attList>
            <attDef ident="type" mode="replace" usage="opt">
              <valList type="closed" mode="replace">
                <valItem ident="uri"/>
                <valItem ident="URLImages"/>
                <valItem ident="shelfmark"/>
                <valItem ident="URLCatalogue"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="bibl" mode="change">
          <attList>
            <attDef ident="type" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="print"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="change" mode="change">
          <attList>
            <attDef ident="when" usage="req" mode="replace"/>
            <attDef ident="who" usage="req" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="correspAction" mode="change">
          <attList>
            <attDef ident="type" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="sent"/>
                <valItem ident="received"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="date" mode="change">
          <attList>
            <!-- doubt: is this necessary? -->
            <attDef ident="when" usage="opt" mode="replace"/>
            <attDef ident="from" usage="opt" mode="replace"/>
            <attDef ident="to" usage="opt" mode="replace"/>
            <attDef ident="notBefore" usage="opt" mode="replace"/>
            <attDef ident="notAfter" usage="opt" mode="replace"/>
            <attDef ident="cert" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="high"/>
                <valItem ident="low"/>
              </valList>
            </attDef>
            <attDef ident="calendar" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="#Julian"/>
                <valItem ident="#Gregorian"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="msDesc" mode="change">
          <attList>
            <attDef ident="type" usage="opt" mode="replace"/>
            <attDef ident="rend" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="print"/>
                <valItem ident="copy"/>
                <valItem ident="manuscript"/>
                <valItem ident="concept"/>
                <valItem ident="notExtant"/>
              </valList>
            </attDef>
            <attDef ident="select" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="#this"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="note" mode="change">
          <attList>
            <attDef ident="type" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="editorial"/>
                <valItem ident="remarkResponsibility"/>
              </valList>
            </attDef>
            <attDef ident="place" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="left"/>
                <valItem ident="right"/>
                <valItem ident="mTop"/>
                <valItem ident="mBottom"/>
                <valItem ident="inline"/>
              </valList>
            </attDef>
            <attDef ident="hand" usage="opt" mode="replace">
              <valList type="open" mode="replace">
                <valItem ident="#author"/>
                <valItem ident="#addressee"/>
                <valItem ident="#unknown"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <!-- doubt: is this necessary? -->
        <elementSpec ident="persName" mode="change">
          <attList>
            <attDef ident="key" usage="opt" mode="replace"/>
            <attDef ident="ref" usage="opt" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="placeName" mode="change">
          <attList>
            <attDef ident="key" usage="req" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="orgName" mode="change">
          <attList>
            <attDef ident="key" usage="req" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="quote" mode="change">
          <attList>
            <attDef ident="rendition" usage="req" mode="replace">
              <valList>
                <valItem ident="#u"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="ref" mode="change">
          <attList>
            <attDef ident="type" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="hasAttachment"/>
                <valItem ident="attachedTo"/>
                <valItem ident="next"/>
                <valItem ident="previous"/>
              </valList>
            </attDef>
            <attDef ident="target" usage="req" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="revisionDesc" mode="change">
          <attList>
            <attDef ident="status" usage="req" mode="replace"/>
          </attList>
        </elementSpec>


        <!-- content and sequence of elements -->
        <elementSpec ident="abstract" mode="change">
          <content>
            <elementRef key="p" minOccurs="1" maxOccurs="unbounded"/>
          </content>
        </elementSpec>


        <!-- elements in text -->
        <elementSpec ident="lb" mode="change">
          <attList>
            <attDef ident="break" usage="opt" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="no"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="pb" mode="change">
          <attList>
            <attDef ident="n" usage="rec" mode="replace"/>
            <attDef ident="facs" usage="opt" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="fw" mode="change">
          <attList>
            <attDef ident="type" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="folNum"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="space" mode="change">
          <attList>
            <attDef ident="unit" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="lines"/>
                <valItem ident="chars"/>
                <valItem ident="words"/>
              </valList>
            </attDef>
            <attDef ident="quantity" usage="req" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="g" mode="change">
          <attList>
            <attDef ident="ref" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="#typoHyphen"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="table" mode="change">
          <attList>
            <attDef ident="rows" usage="req" mode="replace"/>
            <attDef ident="cols" usage="req" mode="replace"/>
            <!--            <attDef ident="rend" usage="req" mode="replace"/>-->
          </attList>
        </elementSpec>
        <elementSpec ident="row" mode="change">
          <attList>
            <attDef ident="role" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="label"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="cell" mode="change">
          <attList>
            <attDef ident="rendition" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="#right"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="hi" mode="change">
          <attList>
            <attDef ident="rendition" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="#u"/>
                <valItem ident="#uu"/>
                <valItem ident="#mMM"/>
                <valItem ident="#sup"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="add" mode="change">
          <attList>
            <attDef ident="place" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="superlinear"/>
                <valItem ident="sublinear"/>
                <valItem ident="intralinear"/>
                <valItem ident="across"/>
                <valItem ident="left"/>
                <valItem ident="right"/>
                <valItem ident="mTop"/>
                <valItem ident="mBottom"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="del" mode="change">
          <attList>
            <attDef ident="rendition" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="#ow"/>
                <valItem ident="#s"/>
                <valItem ident="#erased"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="figure" mode="change">
          <attList>
            <attDef ident="facs" usage="req" mode="replace"/>
            <attDef ident="hand" usage="req" mode="replace"/>
          </attList>
        </elementSpec>
        <elementSpec ident="unclear" mode="change">
          <attList>
            <attDef ident="reason" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="illegible"/>
                <valItem ident="covered"/>
              </valList>
            </attDef>
            <attDef ident="cert" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="low"/>
                <valItem ident="high"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="gap" mode="change">
          <attList>
            <attDef ident="reason" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="lost"/>
                <valItem ident="illegible"/>
                <valItem ident="covered"/>
                <valItem ident="empty"/>
                <valItem ident="fm"/>
                <valItem ident="insignificant"/>
              </valList>
            </attDef>
            <attDef ident="unit" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="chars"/>
                <valItem ident="lines"/>
                <valItem ident="pages"/>
                <valItem ident="words"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="supplied" mode="change">
          <attList>
            <attDef ident="cert" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="low"/>
                <valItem ident="high"/>
              </valList>
            </attDef>
            <!--<attDef ident="reason" mode="delete"/>-->
          </attList>
        </elementSpec>
        <elementSpec ident="seg" mode="change">
          <attList>
            <attDef ident="type" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="comment"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="anchor" mode="change">
          <attList>
            <attDef ident="type" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="target"/>
                <valItem ident="index"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="foreign" mode="change">
          <attList>
            <attDef ident="xml:lang" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="grc"/>
                <valItem ident="lat"/>
                <valItem ident="heb"/>
                <valItem ident="deu"/>
                <valItem ident="fra"/>
                <valItem ident="eng"/>
                <valItem ident="rus"/>
                <valItem ident="esp"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="rs" mode="change">
          <attList>
            <attDef ident="type" usage="req" mode="replace">
              <valList type="closed" mode="replace">
                <valItem ident="term"/>
              </valList>
            </attDef>
          </attList>
        </elementSpec>

        <!-- to improve: 1. make sure usage="opt" is fine, if it does not change, delete; search //attDef[@usage="opt"]  -->
        <!-- to improve: 2. delete non required attributes by using mode="delete" -->
        <!-- For example <attDef ident="reason" mode="delete"/> inside attList -->

      </schemaSpec>
    </body>
  </text>
</TEI>
